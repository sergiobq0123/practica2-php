{
    "name": "miw_upm/miw_api_users_results",
    "description": "MiW REST api",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "homepage": "https://miw.etsisi.upm.es/",
    "keywords": [
        "symfony",
        "doctrine",
        "rest api"
    ],
    "authors": [
        {
            "name": "MiW UPM",
            "homepage": "https://miw.etsisi.upm.es/",
            "email": "miw.etsisi@upm.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-sodium": "*",
        "doctrine/dbal": "^4.2.1",
        "doctrine/doctrine-bundle": "^2.13.1",
        "doctrine/orm": "^3.3",
        "jms/serializer-bundle": "^5.5.1",
        "lexik/jwt-authentication-bundle": "^3.1",
        "symfony/console": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/expression-language": ">=7.2",
        "symfony/flex": "^2.4.7",
        "symfony/framework-bundle": "7.2.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "7.2.*",
        "symfony/security-bundle": ">=7.2",
        "symfony/yaml": "7.2.*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "infection/infection": "^0.29.8",
        "jetbrains/phpstorm-attributes": "^1.2",
        "phpmetrics/phpmetrics": "v3.0.0rc6",
        "phpstan/phpstan": "^1.12.12",
        "phpunit/phpunit": "^9.6.22",
        "rector/rector": "^0.14.8",
        "symfony/browser-kit": ">=7.2",
        "symfony/css-selector": ">=7.2",
        "symfony/maker-bundle": "^1.61",
        "symfony/phpunit-bridge": "^7.2",
        "symfony/stopwatch": ">=7.2",
        "symfony/web-profiler-bundle": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "infection/extension-installer": true
        },
        "bump-after-update": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "infection": "./vendor/bin/infection --ansi --coverage=\"var/coverage\" --threads=20 --only-covered",
        "metrics": "vendor/bin/phpmetrics --junit=\"var/coverage/index.junit.xml\" --report-html=\"docs/PhpMetrics_report\" ./src",
        "phpstan": "./vendor/bin/phpstan --ansi"
    },
    "scripts-descriptions": {
        "infection": "Runs the mutation testing.",
        "metrics": "PhpMetrics provides metrics about PHP project and classes.",
        "phpstan": "PHP Static Analysis"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*",
            "docker": false
        }
    }
}
